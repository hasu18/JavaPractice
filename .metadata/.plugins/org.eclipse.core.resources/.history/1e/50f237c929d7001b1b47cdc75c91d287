package practice;

public class InverseString {

	public static void main(String[] args) {

		String name="harsh";
		int k[]=new int[name.length()];
		for (int i=0;i<name.length();i++) {
			char a=name.charAt(i);
			int j=a;
			k[i]=j;
			System.out.println(j);
		}
for(int j=0;j<k.length;j++) {
	
	System.out.print(k[j]+" ");
	
}	}

}
